@Component
script Zombie_Lupin_Heal extends SkillParent

	@ExecSpace("Client")
	method void ActivateSkill()
		__base:ActivateSkill()
		
		_EffectService:PlayEffectAttached("93f3cd96f3b1432693e5d2765d396338", self.caster, Vector3(0, 0, 0) , 0, Vector3.one)
		self.caster.Unit.hp += self.skillAmount
		
		-- 스킬 사용 종료 처리
		self.caster.Unit.isUsingSkill = false
		self.caster.Unit:SetState("move")
	end

end