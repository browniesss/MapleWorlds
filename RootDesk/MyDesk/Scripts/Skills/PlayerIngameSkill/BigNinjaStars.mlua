@Component
script BigNinjaStars extends Component

	property number damage = 0

	property number time = 0

	property number lifeTime = 0

	@ExecSpace("ClientOnly")
	method void OnUpdate(number delta)
		self.Entity.TransformComponent.Position = self.Entity.TransformComponent.Position + Vector3(1, 0, 0) * 2
		
		self.time += delta;
		if self.time >= self.lifeTime then
			self.Entity:Destroy()
		end
	end

end