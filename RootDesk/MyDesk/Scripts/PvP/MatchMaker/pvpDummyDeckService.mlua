@Logic
script pvpDummyDeckService extends Logic

	property table deckList = {}

	property table nicknamePool = {}

	method void OnBeginPlay()
		self:InitDecks()
	end

	@ExecSpace("ServerOnly")
	method void InitDecks()
		self.deckList = {bronze = {}, silver = {}, gold = {}, platinum = {}, diamond = {}}
		self:InitBronzeDecks()
		self:InitSilverDecks()
		self:InitGoldDecks()
		self:InitPlatinumDecks()
		self:InitDiamondDecks()
		self:InitNicknames()
	end

	@ExecSpace("ServerOnly")
	method table BuildDeck(table units, integer minLevel, integer maxLevel, integer minLB, integer maxLB)
		local deck = {}
		local info = {}
		for _, u in ipairs(units) do
			table.insert(deck, u[1])
			info[u[1]] = {
				id = u[1],
				level = math.random(minLevel, maxLevel),
				limitBreak = math.random(minLB, maxLB),
				cost = u[2]
			}
		end
		return {
			defense_deck = deck,
			unitInfo = info,
			defense_synergy = self:ComputeDummySynergy(deck)
		}
	end

