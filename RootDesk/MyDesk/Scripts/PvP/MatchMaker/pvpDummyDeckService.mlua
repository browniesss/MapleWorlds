	@ExecSpace("ServerOnly")
	method table ComputeDummySynergy(table deck)
		local countMap = {}
		local cols = {"jobId", "countryId", "regionId"}
		for _, unitId in ipairs(deck) do
			local unit = _UnitService:GetUnitByIDFromServer(unitId)
			if unit == nil then
				continue
			end
			for _, col in ipairs(cols) do
				if unit[col] ~= nil and unit[col] ~= 0 then
					local key = tostring(unit[col])
					if countMap[key] == nil then
						countMap[key] = 0
					end
					countMap[key] = countMap[key] + 1
				end
			end
		end
		return countMap
	end

	@ExecSpace("ServerOnly")
	method string GetTierNameFromScore(integer score)
		if score >= _pvpRankingService.dia then
			return "diamond"
		elseif score >= _pvpRankingService.platinum then
			return "platinum"
		elseif score >= _pvpRankingService.gold then
			return "gold"
		elseif score >= _pvpRankingService.silver then
			return "silver"
		end
		return "bronze"
	end

	@ExecSpace("ServerOnly")
	method table GetDummyDeckByTier(string tierName)
		local tierDecks = self.deckList[tierName]
		if tierDecks == nil or #tierDecks == 0 then
			self:InitDecks()
			tierDecks = self.deckList[tierName]
			if tierDecks == nil or #tierDecks == 0 then
				tierDecks = self.deckList.bronze
			end
		end
		local index = math.random(1, #tierDecks)
		return tierDecks[index]
	end

	@ExecSpace("ServerOnly")
	method string GetRandomNicknameByTier(string tierName)
		local names = self.nicknamePool[tierName]
		if names == nil or #names == 0 then
			self:InitNicknames()
			names = self.nicknamePool[tierName]
			if names == nil or #names == 0 then
				return "MapleBot"
			end
		end
		local index = math.random(1, #names)
		return names[index]
	end

